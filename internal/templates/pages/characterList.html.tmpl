{{define "content"}}
<div class="flex flex-col gap-4">
    <a href="/character/new" class="bg-primary p-3 rounded-2xl max-w-fit">New Character</a>
    <div class="h-full overflow-auto">
        {{range .}}
        <p>{{.Name}}</p>
        {{else}}
        <p>No Characters found.</p>
        {{end}}
    </div>
</div>
{{end}}